let person = {
  first_name : "Muhammad",
  last_name : "Tahir",
  // method
  getFunction : function(){
    return (`The name of the person is 
    ${person.first_name} ${person.last_name}`)
  },
  // object within object 
  phone_number : {
    mobile : '34232435',
    land_line : '325246587',
  }
}
console.log(person.getFunction());
console.log(person.phone_number.mobile);

